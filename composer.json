{
    "name": "chriskonnertz/jobs",
    "description": "Simple cron job manager",
    "keywords": ["cron jobs", "cron job", "job", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Konnertz"
        }
    ],
    "require": {
        "php": ">=5.3.7"
    },
    "autoload": {
        "psr-4": {
            "ChrisKonnertz\\Jobs\\": "src/ChrisKonnertz/Jobs"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ChrisKonnertz\\Jobs\\Integration\\JobsServiceProvider"
            ],
            "aliases": {
                "OpenGraph": "ChrisKonnertz\\Jobs\\Integration\\JobsFacade"
            }
        }
    },
    "minimum-stability": "dev"
}
